{
  "name": "assignment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=dev PORT=3001 node dist/bin/www",
    "babel-node": "./node_modules/.bin/babel-node",
    "test": "mocha test/functional/api/agileTest.js",
    "compile": "babel ./ --out-dir dist/ --ignore ./node_modules,./test --copy-files",
    "build": "npm run compile",
    "clean": "rimraf ./dist && mkdir dist",
    "postbuild": "npm start",
    "prebuild": "npm-run-all test clean",
    "start:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "lint": "esw  ./test ./routes ./models/ app.js bin/www ",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "body-parser": "~1.13.2",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "^4.13.4",
    "fuse.js": "^3.4.5",
    "http-error": "0.0.6",
    "http-errors": "^1.7.3",
    "mongoose": "^5.7.3",
    "morgan": "^1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-watch": "^6.0.1",
    "json-server": "^0.15.1",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^5.2.11",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  }
}
